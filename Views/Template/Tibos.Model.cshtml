@model TableConfig

@{ 
    string functionStr = Model.TableName.Substring(0, 1).ToLower() + Model.TableName.Substring(1);
}

    <pre>
using System;
using System.ComponentModel.DataAnnotations.Schema;

namespace @Model.NameSapce
{
    [Table("@functionStr")]
	//@Model.FullName;
	public class @Model.FullName:BaseEntity
    {
    @foreach (var item in Model.ColumnConfig)
    {
        if (item.PropName == "Id") { continue; }

        <text>
        /// <summary>
		/// @Html.Raw(item.Remark)
        /// </summary>
        public virtual @item.CsType @item.PropName
        {
            get; 
            set; 
        }  
        </text>
    }

    }
}
</pre>